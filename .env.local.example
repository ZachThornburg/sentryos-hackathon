# Sentry Configuration
# Copy this file to .env.local and fill in your actual values
# Get your DSN from: https://sentry.io/settings/projects/your-project/keys/

# Public DSN (safe to expose to the browser)
NEXT_PUBLIC_SENTRY_DSN=https://your-key@o0.ingest.sentry.io/0000000

# Server-side DSN (same as public DSN, but kept separate for clarity)
SENTRY_DSN=https://your-key@o0.ingest.sentry.io/0000000

# Sentry Organization slug (for source map uploads)
SENTRY_ORG=your-org-slug

# Sentry Project name (for source map uploads)
SENTRY_PROJECT=your-project-name

# Sentry Auth Token (for uploading source maps during build)
# Get this from: https://sentry.io/settings/account/api/auth-tokens/
SENTRY_AUTH_TOKEN=your-auth-token-here
